<div>
  <h2><strong>Lab Results</strong></h2>
  {{#each lab}}
<div class="lab-card">
    <ul class="list-group list-group-flush">
      <button type="click" class="delete-button text-danger">Delete</button>
      <li class="list-group-item">{{name}}</li>
      <li class="list-group-item">{{content}}</li>
    </ul>
</div>
<form class="form">
    <input type="text" name="name" placeholder="Lab Name" />
    <input type="text" name="specialty" placeholder="Lab Content" />

    <button
      type="submit"
      class="create btn btn-outline-success btn-sm"
    >Create</button>
  </form>
</div>

<script src="/js/lab.js"></script>
<script src="/js/lab_delete.js"></script>
