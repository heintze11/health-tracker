<div>
   <h2>Prescriptions</h2>
 <div class="card bg-light mb-3" style="max-width: 18rem;">
         {{#each prescription}}
  <div class="card-header">Prescription <button class="btn-trash">ðŸ—‘</button></div>
  <div class="card-body">
    <h5 class="card-title"></h5>
    <p class="card-text">
      <ul class="list-group list-group-flush">
         <li class="list-group-item">Prescription Name: {{name}}</li>
         <li class="list-group-item">Recommended Dose: {{dose}}</li>
         <li class="list-group-item">Dosage Frequency: {{frequency}}</li>
      </ul>
   </p>
  </div>
         {{/each}}
</div>
<form class="form">
    <input type="text" name="name" placeholder="Prescription Name" />
    <input type="text" name="dose" placeholder="Recommended Dose" />
    <input type="text" name="frequency" placeholder="Dosage Frequency" />

    <button
      type="submit"
      class="create btn btn-outline-success btn-sm"
    >Create</button>
  </form>
</div>

<script src="/js/prescription.js"></script>